{
  "name": "icpdao-interface",
  "private": true,
  "scripts": {
    "graphql:generate:dao": "SERVICE=dao graphql-codegen -r dotenv/config --config codegen.yml",
    "graphql:generate:uniswap": "graphql-codegen --config codegen_uniswap.yml",
    "graphql:generate:subgraph": "graphql-codegen --config codegen_subgraph.yml",
    "graphql:generate": "yarn graphql:generate:dao && yarn graphql:generate:uniswap && yarn graphql:generate:subgraph",
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "yarn graphql:generate && umi build",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "yarn graphql:generate && cross-env UMI_ENV=dev umi dev",
    "start:dev": "yarn graphql:generate && cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "yarn graphql:generate && cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "yarn graphql:generate && cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "yarn graphql:generate && cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "yarn graphql:generate && cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-descriptions": "^1.6.8",
    "@ant-design/pro-form": "^1.18.3",
    "@ant-design/pro-layout": "^6.15.3",
    "@ant-design/pro-table": "^2.30.8",
    "@antv/l7": "^2.5.12",
    "@antv/l7-react": "^2.3.3",
    "@apollo/client": "^3.3.19",
    "@aws-sdk/client-s3": "^3.16.0",
    "@metamask/detect-provider": "^1.2.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@sentry/react": "^6.13.2",
    "@sentry/tracing": "^6.13.2",
    "@types/rimraf": "^3.0.0",
    "@umijs/openapi": "^1.1.14",
    "@umijs/plugin-openapi": "^1.2.0",
    "@umijs/route-utils": "^1.0.36",
    "@uniswap/default-token-list": "^2.0.0",
    "@uniswap/token-lists": "^1.0.0-beta.26",
    "@uniswap/v3-periphery": "^1.2.0",
    "@web3-react/core": "^6.1.9",
    "ahooks": "^2.10.9",
    "antd": "^4.14.0",
    "apollo-boost": "^0.4.9",
    "classnames": "^2.2.6",
    "ethers": "^5.4.4",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "lodash": "^4.17.11",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "omit.js": "^2.0.2",
    "rc-animate": "^3.1.1",
    "rc-banner-anim": "^2.4.5",
    "rc-queue-anim": "^2.0.0",
    "rc-scroll-anim": "^2.7.6",
    "rc-texty": "^0.2.0",
    "rc-tween-one": "^2.7.3",
    "react": "^17.0.0",
    "react-apollo": "^3.1.5",
    "react-apollo-hooks": "^0.5.0",
    "react-dev-inspector": "^1.1.1",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.0.4",
